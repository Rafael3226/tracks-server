// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Track {
  id           String  @id
  title        String
  artist       String
  label        String
  imageUrl     String
  genre        String
  isDownloaded Boolean @default(false)
}
